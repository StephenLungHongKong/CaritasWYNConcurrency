@model CaritasWYN.Models.JobDuty

@{
    ViewData["Title"] = "Delete";
}


<div class="jumbotron">
    <div class="d-flex justify-content-center">
        <div class="row">
            <div class="col-md-8">
                <h1>Delete</h1>
                <h3 class="text-danger">Are you sure you want to delete this?</h3>
                <hr />
            </div>

            <div class="col-md-8">
                @Html.DisplayFor(model => model.JobType)
                <hr />
                <form asp-action="Delete">
                    <input type="hidden" asp-for="JobDutyId" />
                    <input type="submit" value="Delete" class="btn btn-danger" />
                    <a asp-action="Index" class="btn btn-outline-danger">Cancel</a>
                </form>

            </div>

        </div>
    </div>
</div>